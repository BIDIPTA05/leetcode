class Solution {
public:
    int findNumbers(vector<int>& nums) {
       
      int a=0;
        int n = nums.size();
        for(int i = 0 ; i< n ; i++){
            //counting total number of letter inside each array element
             int even=0;
            while(nums[i]!=0)
            {
            nums[i]/=10;
            even++;
            }
                if(even%2==0){
                    a++;
                }
        
            
        };
        return a;
    }
};
