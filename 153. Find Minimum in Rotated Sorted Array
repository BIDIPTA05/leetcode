stl-
class Solution {
public:
    int findMin(vector<int>& nums) {
       int min = *min_element(nums.begin(), nums.end());
        return min;
    }
};



*******************************************************

use sort inbuilt function-

class Solution {
public:
    int findMin(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int a = nums[0];
        return a;         
    }
};
